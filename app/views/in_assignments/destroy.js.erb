replaceElement('.modal-in-assignment-list', "<%= j render 'dashboard/IN/components/in_assignment_modal/detail_load_in_in_assignment_rows' %>");
replaceElement(('#<%= dom_id(@load_in) %>'), "<%= j render 'dashboard/IN/components/load_in_modal/detail_load_in_global' %>");
replaceElement(('#load_in_row_single-<%=(@load_in.id)%>'), "<%= j render 'dashboard/IN/components/load_in_row_single' %>");
var insertHere = document.querySelectorAll('#accordeon-load_in-<%=(@load_in.id)%>');
insertHere.forEach((node) => { node.remove();});
var loadin = document.querySelector('#load_in_row_single-<%=(@load_in.id)%>');
loadin.insertAdjacentHTML('afterend',"<%= j render 'dashboard/IN/components/in_assignments_rows' %>" );
insertHere.forEach((node) => { node.classList.add("show");});
