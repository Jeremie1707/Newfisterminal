replaceElement('.modal-out-assignment-list', "<%= j render 'dashboard/OUT/components/out_assignment_modal/detail_load_out_out_assignment_rows' %>");
replaceElement(('#<%= dom_id(@load_out) %>'), "<%= j render 'dashboard/OUT/components/load_out_modal/detail_load_out_global' %>");
replaceElement(('#load_out_row_single-<%=(@load_out.id)%>'), "<%= j render 'dashboard/OUT/components/load_out_row_single' %>");

var insertHere = document.querySelectorAll('#accordeon-load_out-<%=(@load_out.id)%>');
insertHere.forEach((node) => { node.remove();});
var loadout = document.querySelector('#load_out_row_single-<%=(@load_out.id)%>');
loadout.insertAdjacentHTML('afterend',"<%= j render 'dashboard/OUT/components/out_assignments_rows' %>" );
insertHere.forEach((node) => { node.classList.add("show");});


